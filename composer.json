{
    "name": "amukhin-cyberhull/nova-comments",
    "description": "A Laravel Nova resource for commenting on model.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "homepage": "https://github.com/amukhin-cyberhull/nova-comments",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Ferens",
            "email": "brandon@kirschbaumdevelopment.com",
            "role": "Developer"
        },
        {
            "name": "Andrey Mukhin",
            "email": "centgart@gmail.com",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\NovaComments\\": "src/",
            "KirschbaumDevelopment\\NovaComments\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\NovaComments\\NovaCommentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
